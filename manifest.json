{
    "manifest_version": 3,
    "name": "AI Text Enhancer",
    "version": "1.0",
    "description": "An extension that connects to an API and displays a tooltip.",
    "permissions": [
        "storage",
        "activeTab",
        "identity"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon416.png",
        "32": "icons/icon432.png",
        "128": "icons/icon4128.png"
    }
}